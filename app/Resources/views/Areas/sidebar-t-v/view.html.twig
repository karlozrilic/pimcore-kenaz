<a class="aside-tv-link" href="{{ video.link }}" target="_blank">
    <iframe class="aside-tv-video" src="{{ video.link }}" frameborder="0" 
    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen>
    </iframe>
    <div class="aside-tv-text">
        <div class="aside-tv-date">{{ video.date|date('F m, Y') }}</div>
        <div class="aside-tv-article-title">{{ video.title }}</div>
    </div>
</a>